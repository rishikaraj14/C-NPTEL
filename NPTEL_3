#include <iostream>
using namespace std;
class Student {
    public : 
      int id ;
      string name;
      void insert (int i , string n){
        id = i ;
        name = n ;
    }
      void display (){
        cout<<id<<"-"<<name<<endl;
    }
};

int main() { 
Student s1;
Student s12;
s1.insert(101,"Rishika");
s2.insert(102 ,"Shreya")'
s1.display();
s1.dispaly();
return 0 ;
}
